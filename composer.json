{
    "name": "bref/dynamap",
    "description": "DynamoDB object mapper",
    "keywords": ["dynamodb", "mapper", "orm", "aws", "object", "bref"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Dynamap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynamap\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.130",
        "symfony/property-info": "^4.2",
        "phpdocumentor/reflection-docblock": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mnapoli/hard-mode": "^0.3"
    }
}
